@model IEnumerable<ET.WebDirectory>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MusicLayout.cshtml";
}

<section id="one" class="wrapper style2">
    <div class="inner">
        <div class="grid-style">
            @foreach (var item in Model)
            {
                Html.RenderAction("_MainMenu", new { MainClass = item.MainClass, ImagePathNumber = item.ProfileID });
            }
        </div>
    </div>
</section>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}